{
  "name": "deep-objectify",
  "version": "1.0.3",
  "description": "A simple utility for converting a deep JSON structure to pure nested objects",
  "main": "dist/deepObjectify.js",
  "typings": "dist/deepObjectify.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest deepObjectifyTests.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terraindata/deep-objectify.git"
  },
  "keywords": [
    "object",
    "arrays",
    "nested",
    "deep",
    "objectify"
  ],
  "author": "Terrain Data, Inc.",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/terraindata/deep-objectify/issues"
  },
  "homepage": "https://github.com/terraindata/deep-objectify#readme",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ],
    "testRegex": "/.+Tests\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "is-primitive": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
